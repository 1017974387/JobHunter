<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd">


<mapper namespace="com.cszjo.jobhunter.dao.ClawerTaskDao">

    <resultMap id="clawerTaskResult" type="com.cszjo.jobhunter.model.ClawerTask">
        <id property="id" column="id"/>
        <result property="taskName" column="task_name"/>
        <result property="url" column="url"/>
        <result property="jobCount" column="job_count"/>
        <result property="statu" column="statu"/>
        <result property="createTime" column="create_time"/>
        <result property="userId" column="user_id"/>
        <result property="threadCount" column="thread_count"/>
        <result property="cityKey" column="city_key"/>
        <result property="cityValue" column="city_value"/>
        <result property="keyWordKey" column="key_word_key"/>
        <result property="keyWordValue" column="key_word_value"/>
    </resultMap>
    
    <parameterMap id="clawerTaskParam" type="com.cszjo.jobhunter.model.ClawerTask">
        <parameter property="id" resultMap="clawerTask" />
        <parameter property="taskName" resultMap="clawerTask" />
        <parameter property="url" resultMap="clawerTask" />
        <parameter property="jobCount" resultMap="clawerTask" />
        <parameter property="statu" resultMap="clawerTask" />
        <parameter property="createTime" resultMap="clawerTask" />
        <parameter property="userId" resultMap="clawerTask" />
        <parameter property="threadCount" resultMap="clawerTask" />
        <parameter property="cityKey" resultMap="clawerTask" />
        <parameter property="cityValue" resultMap="clawerTask" />
        <parameter property="keyWordKey" resultMap="clawerTask" />
        <parameter property="keyWordValue" resultMap="clawerTask" />
    </parameterMap>

    <select id="selectAll" resultMap="clawerTaskResult">
        SELECT * FROM clawer_task;
    </select>

    <insert id="addTask" parameterMap="clawerTaskParam">
        INSERT INTO clawer_task(task_name,url,job_count,statu,create_time,user_id,thread_count,city_key,city_value,key_word_key,key_word_value)
            VALUES(#{task_name},#{url},#{job_count},#{statu},#{create_time},#{user_id},#{thread_count},#{city_key},#{city_value},#{key_word_key},#{key_word_value});
    </insert>
</mapper>